{
  "name": "aws-cdk-dynamodb-seeder",
  "version": "1.2.1",
  "description": "A simple CDK JSON seeder for DynamoDB",
  "scripts": {
    "build": "tsc",
    "check": "npm test && npm run lint",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm run check",
    "preversion": "npm run lint",
    "test": "jest",
    "version": "npm run format && git add -A src"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elegantdevelopment/aws-cdk-dynamodb-seeder.git"
  },
  "keywords": [
    "aws",
    "cdk",
    "dynamodb",
    "seed",
    "seeder"
  ],
  "author": "Justin Taylor <jtaylor@elegantdevelopment.co.uk> (https://www.linkedin.com/in/jsdtaylor)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/elegantdevelopment/aws-cdk-dynamodb-seeder/issues"
  },
  "homepage": "https://github.com/elegantdevelopment/aws-cdk-dynamodb-seeder#readme",
  "dependencies": {
    "@aws-cdk/aws-dynamodb": "^1.25.0",
    "@aws-cdk/aws-lambda": "^1.25.0",
    "@aws-cdk/aws-s3": "^1.25.0",
    "@aws-cdk/aws-s3-deployment": "^1.25.0",
    "@aws-cdk/core": "^1.25.0",
    "@aws-cdk/custom-resources": "^1.25.0",
    "aws-sdk": "^2.621.0",
    "tmp": "^0.1.0"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.25.0",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.2",
    "@types/tmp": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.ts",
      "!node_modules/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      },
      "testEnvironment": "node"
    }
  },
  "files": [
    "lib/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run check",
      "pre-push": "npm run check"
    }
  }
}
